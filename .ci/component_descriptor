#!/usr/bin/env sh
set -e
if [ -z "$SOURCE_PATH" ]; then
  SOURCE_PATH="$(readlink -f $(dirname ${0})/..)"
fi
${ADD_DEPENDENCIES_CMD} \
  --container-image-dependencies \
  '{"name": "pause-container", "image_reference": " gcr.io/google_containers/pause-amd64", "version": "$(cat ${SOURCE_PATH}/PAUSE_CONTAINER_VERSION)" }'